<script>

	console.log(TaskPhasData);;
	console.log(DefectTypeData);
	console.log(DefectStateDate);
	console.log(DefectResponsibilityTeamDate);
	console.log(ChangeRequestDate);
	console.log(ChangeRequestStateDate);
	console.log(ChangeOrderDate);
	
    getRiskDate();
	getDefectDate();
	getTaskPhasData();
	getDefectStateDate();
	getDefectResponsibilityTeamDate();
	getChangeRequestDate();
	getChangeRequestStateDate();
	getChangeOrderDate();
	
	//给html赋值
	function getChangeOrderDate(){
		var html = '';
		for(var key in ChangeOrderDate){
			var html1 = '<tr class="odd_row" style="width: 100%;"><th style="text-align:right; width: 30%">'+key+ ':</th>';
			var html2 = "";
			var arr = ChangeOrderDate[key];
			for(var i =0;i<arr.length;i++){
				var obj = arr[i];
				//console.log(Object.keys(obj)[0]);
				//onsole.log(Object.values(obj)[0]);
			
				html2 += '<td style="text-align:center; "> '+Object.values(obj)[0]+'</td>';
			}			
			html += html1+html2+'</tr>';
		}
		document.getElementById("ChangeOrderState").innerHTML = html;
	}
	
	function getChangeRequestStateDate(){
		var html = '';
		for(var key in ChangeRequestStateDate){
			var html1 = '<tr class="odd_row" style="width: 100%;"><th style="text-align:right; width: 30%">'+key+ ':</th>';
			var html2 = "";
			var arr = ChangeRequestStateDate[key];
			for(var i =0;i<arr.length;i++){
				var obj = arr[i];
				//console.log(Object.keys(obj)[0]);
				//onsole.log(Object.values(obj)[0]);
			
				html2 += '<td style="text-align:center; "> '+Object.values(obj)[0]+'</td>';
			}			
			html += html1+html2+'</tr>';
		}
		document.getElementById("ChangeRequestState").innerHTML = html;
	}
	
	function getChangeRequestDate(){
