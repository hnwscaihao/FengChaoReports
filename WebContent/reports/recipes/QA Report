    //把QA Task的数据写入对应table
    function getQATaskData(){
        var html = '';
        for(var key in QATaskData){
            var html1 = '<tr class="odd_row" style="width: 100%;"><th style="text-align:right; width: 30%">'+key+ ':</th>';
            var html2 = "";
            var arr = QATaskData[key];
            for(var i =0;i<arr.length;i++){
                var obj = arr[i];
                html2 += '<td style="text-align:center; "> '+Object.values(obj)[0]+'</td>';
            }
            html += html1+html2+'</tr>';
        }
        document.getElementById("QATaskDataClo").innerHTML = html;
    };

    //把...Docuemnt的数据写入table
    function getDocData(){
        var html = '';
        for(var key in DocTypeData){
            var html1 = '<tr class="odd_row" style="width: 100%;"><th style="text-align:right; width: 30%">'+key+ ':</th>';
            var html2 = "";
            var arr = DocTypeData[key];
            for(var i =0;i<arr.length;i++){
                var obj = arr[i];
                console.log(Object.keys(obj)[0]);
                console.log(Object.values(obj)[0]);

                html2 += '<td style="text-align:center; "> '+Object.values(obj)[0]+'</td>';
            }
            html += html1+html2+'</tr>';
        }
        document.getElementById("docTyAndSta").innerHTML = html;

    }

