<script type="text/javascript">

    //获取QA Task数据
    if("<%<%builtin Type%>%>" == "QA Task"){
        var arr = QATaskData["Count"];
        for(var i=0;i<arr.length;i++){
            var obj = arr[i];
            if ("<%State%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%State%>"]);
                obj["<%State%>"] = count + 1;
            }
        }
    }

    //获取...Document数据
    if("<%<%builtin Type%>%>".indexOf("Document") > -1 ){
        for(var key in DocTypeData){
            if("<%State%>" == key){
                var arr = DocTypeData[key];
                for(var i = 0;i<arr.length;i++){
                    var obj = arr[i];
                    if("<%Type%>" == Object.keys(obj)[0]){
                        var count = parseInt(obj["<%Type%>"]);
                        obj["<%Type%>"] = count+1;
                    }
                }
            }
        }
    }

    if("<%<%builtin Type%>%>" == "Defect"){
        for(var key in DefectBySourceData){
            if("<%Defect Source%>" == key){
                var arr = DefectBySourceData[key];
                for(var i = 0;i<arr.length;i++){
                    var obj = arr[i];
                    if("<%Defect Type%>" == Object.keys(obj)[0]){
                        var count = parseInt(obj["<%Defect Type%>"]);
                        obj["<%Defect Type%>"] = count+1;
                    }
                }
            }
        }
    }


    //Defect statistics by Test Level
    if("<%<%builtin Type%>%>" == "Defect"){
        for(var key in defectTestLevelData){
            if ("<%Origin%>" == key){
                var arr = defectTestLevelData[key];
                for(var i = 0;i<arr.length;i++){
                    var obj = arr[i];
                    if("<%Task Phase%>" == Object.keys(obj)[0]){
                        var count = parseInt(obj["<%Task Phase%>"]);
                        obj["<%Task Phase%>"] = count + 1;
                    }
                }
            }
        }
    }

    //Delayed Defect statistics
/*    if("<%<%builtin Type%>%>" == "Defect"){
        for(var key in delayedDefectData){
            if ("<%%>" == key){
                var arr = [key];
                for(var i = 0;i<arr.length;i++){
                    var obj = arr[i];
                    if("<%%>" == Object.keys(obj)[0]){
                        var count = parseInt(obj["<%%>"]);
                        obj["<%%>"] = count + 1;
                    }
                }
            }
        }
    }*/

    //Defect state by Responsibility Team Table
    if("<%<%builtin Type%>%>" == "Defect"){
        for(var key in defectResponsibilityTeamData){
            if ("<%Responsibility Team%>" == key){
                var arr = defectResponsibilityTeamData[key];
                for(var i = 0;i<arr.length;i++){
                    var obj = arr[i];
                    if("<%State%>" == Object.keys(obj)[0]){
                        var count = parseInt(obj["<%State%>"]);
                        obj["<%State%>"] = count + 1;
                    }
                }
            }
        }
    }

    //Test Plan Statistics
    if("<%<%builtin Type%>%>" == "Test Plan"){
        var arr = TestPlanData["Count"];
        for(var i=0;i<arr.length;i++){
            var obj = arr[i];
            if ("<%State%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%State%>"]);
                obj["<%State%>"] = count + 1;
            }
        }
    }

    //Test Objective statistics
    if("<%<%builtin Type%>%>" == "Test Objective"){
        var arr = TestObjectiveData["Count"];
        for(var i=0;i<arr.length;i++){
            var obj = arr[i];
            if ("<%State%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%State%>"]);
                obj["<%State%>"] = count + 1;
            }
        }
    }

    //Test Session statistics
    if("<%<%builtin Type%>%>" == "Test Session"){
        var arr = TestSessionData["Count"];
        for(var i = 0;i<arr.length;i++){
            var obj = arr[i];
            if("<%State%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%State%>"]);
                obj["<%State%>"] = count + 1;
            }
        }
    }

    if("<%<%builtin Type%>%>" == "Change Request"){
        for(var i=0;i<ChangeRequestStateData.length;i++){
            var obj = ChangeRequestStateData[i];
            var key = Object.keys(obj)[0];
            var count = parseInt(obj[key]);
            if("<%State%>" == key){
                obj[key] = count+1;
            }
        }
    }

    //Change Request by Category
    if("<%<%builtin Type%>%>" == "Change Request"){
        for(var key in ChangeRequestCategoryData){
            if ("<%Change Category%>" == key){
                var arr = ChangeRequestCategoryData[key];
                for(var i = 0;i<arr.length;i++){
                    var obj = arr[i];
                    if("<%Change Initiator%>" == Object.keys(obj)[0]){
                        var count = parseInt(obj["<%Change Initiator%>"]);
                        obj["<%Change Initiator%>"] = count + 1;
                    }
                }
            }
        }
    }

    if("<%<%builtin Type%>%>" == "Change Order"){
        for(var i=0;i<ChangeOrderStateData.length;i++){
            var obj = ChangeOrderStateData[i];
            var key = Object.keys(obj)[0];
            var count = parseInt(obj[key]);
            if("<%State%>" == key){
                obj[key] = count+1;
            }
        }
    }

    //Task statistics by State
    if("<%<%builtin Type%>%>" == "Task"){
        var arr = TaskbyStateData["Task"];
        for(var i = 0;i<arr.length;i++){
            var obj = arr[i];
            if("<%State%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%State%>"]);
                obj["<%State%>"] = count + 1;
            }
        }
    }

    //Task by Responsibility Team
    if("<%<%builtin Type%>%>" == "Task"){
        var arr = TaskbyRespTeamData["Task"];
        for(var i = 0;i<arr.length;i++){
            var obj = arr[i];
            if("<%Responsibility Team%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%Responsibility Team%>"]);
                obj["<%Responsibility Team%>"] = count + 1;
            }
        }
    }

    //Supplier Monitor Task Statistics by state
    if("<%<%builtin Type%>%>" == "Supplier Monitor Task"){
        var arr = SupplMonTaskStateData["Supplier Monitor Task"];
        for(var i = 0;i<arr.length;i++){
            var obj = arr[i];
            if("<%State%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%State%>"]);
                obj["<%State%>"] = count + 1;
            }
        }
    }

    //Supplier Monitor Task Statistics by Responsibility Team
    if("<%<%builtin Type%>%>" == "Supplier Monitor Task"){
        var arr = SupplMonTaskRespTeamData["Supplier Monitor Task"];
        for(var i = 0;i<arr.length;i++){
            var obj = arr[i];
            if("<%Responsibility Team%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%Responsibility Team%>"]);
                obj["<%Responsibility Team%>"] = count + 1;
            }
        }
    }

	//Release Statistics
    if("<%<%builtin Type%>%>" == "Release"){
        var arr = ReleaseData["Release"];
        for(var i = 0;i<arr.length;i++){
            var obj = arr[i];
            if("<%State%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%State%>"]);
                obj["<%State%>"] = count + 1;
            }
        }
    }

    //Risk Statistics
    if("<%<%builtin Type%>%>" == "Risk"){
        var arr = RiskData["Risk"];
        for(var i = 0;i<arr.length;i++){
            var obj = arr[i];
            if("<%State%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%State%>"]);
                obj["<%State%>"] = count + 1;
            }
        }
    }

	if("<%<%builtin Type%>%>" == "Non-Conformance Issue"){
        for(var i=0;i<NonConfIssProcAreaData.length;i++){
            var obj = NonConfIssProcAreaData[i];
            var key = Object.keys(obj)[0];
            var count = parseInt(obj[key]);
            if("<%Process Area%>" == key){
                obj[key] = count+1;
            }
        }
    }

    if("<%<%builtin Type%>%>" == "Non-Conformance Issue"){
        var arr = NonConfIssRespTeamData["Non-Conformance Issue"];
        for(var i = 0;i<arr.length;i++){
            var obj = arr[i];
            if("<%Responsibility Team%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%Responsibility Team%>"]);
                obj["<%Responsibility Team%>"] = count + 1;
            }
        }
    }

	if("<%<%builtin Type%>%>" == "LL and BP"){
        var arr = LLandBPStateData["LL and BP"];
        for(var i = 0;i<arr.length;i++){
            var obj = arr[i];
            if("<%State%>" == Object.keys(obj)[0]){
                var count = parseInt(obj["<%State%>"]);
                obj["<%State%>"] = count + 1;
            }
        }
    }


</script>



