	function writeNonConfIssProcAreaData(){
        for(var i=0;i<NonConfIssProcAreaData.length;i++){
            var obj = NonConfIssProcAreaData[i];
            var key = Object.keys(obj)[0];
            var count = parseInt(obj[key]);
            if("ACQ.4 Supplier monitoring" == key){
                document.getElementById("ACQ4").innerHTML = count;
            }else if("SPL.2 Product release" == key){
                document.getElementById("SPL2").innerHTML = count;
            }else if("SYS.1 Requirement elicitation" == key){
                document.getElementById("SYS1").innerHTML = count;
            }
            else if("SYS.2 System requirement analysis" == key){
                document.getElementById("SYS2").innerHTML = count;
            }
            else if("SYS.3 System architectural design" == key){
                document.getElementById("SYS3").innerHTML = count;
            }else if("SYS.4 System integration and integration test" == key){
                document.getElementById("SYS4").innerHTML = count;
            }else if("SYS.5 System qualification test" == key){
                document.getElementById("SYS5").innerHTML = count;
            }else if("SWE.1 Software requirement analysis" == key){
                document.getElementById("SWE1").innerHTML = count;
            }
            else if("SWE.2 Software architectural design" == key){
                document.getElementById("SWE2").innerHTML = count;
            }
            else if("SWE.3 Software detailed design and unit construction" == key){
                document.getElementById("SWE3").innerHTML = count;
            }else if("SWE.4 Software unit verification" == key){
                document.getElementById("SWE4").innerHTML = count;
            }else if("SWE.5 Software integration and integration test" == key){
                document.getElementById("SWE5").innerHTML = count;
            }else if("SWE.6 Software qualification test" == key){
                document.getElementById("SWE6").innerHTML = count;
            }
            else if("SUP.1 Quality assurance" == key){
                document.getElementById("SUP1").innerHTML = count;
            }
            else if("SUP.2 Verification" == key){
                document.getElementById("SUP2").innerHTML = count;
            }else if("SUP.4 Joint Review" == key){
                document.getElementById("SUP4").innerHTML = count;
            }else if("SUP.8 Configuration management" == key){
                document.getElementById("SUP8").innerHTML = count;
            }else if("SUP.9 Problem resolution management" == key){
                document.getElementById("SUP9").innerHTML = count;
            }
            else if("SUP.10 Change request management" == key){
                document.getElementById("SUP10").innerHTML = count;
            }
            else if("MAN.3 Project management" == key){
                document.getElementById("MAN3").innerHTML = count;
            }else if("MAN.5 Risk management" == key){
                document.getElementById("MAN5").innerHTML = count;
            }else if("MAN.6 measurement" == key){
                document.getElementById("MAN6").innerHTML = count;
            }else if("REU.2 Reuse program management" == key){
                document.getElementById("REU2").innerHTML = count;
            }
            else if("HWE.1 Hardware requirement analysis" == key){
                document.getElementById("HWE1").innerHTML = count;
            }
            else if("Total" == key){
                document.getElementById("TotalClo").innerHTML = count;
            }

        }
    };

