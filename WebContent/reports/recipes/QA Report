    function getTaskbyStateData(){
        var html = '';
        for (var key in TaskbyStateData){
            var html1 = '<tr class="odd_row" style="width: 100%;"><th style="text-align:right; width: 30%">'+ key +'</th>';
            var html2 = '';
            var arr = TaskbyStateData[key];
            for(var i =0;i<arr.length;i++){
                var obj = arr[i];
                html2 += '<td style="text-align:center; "> '+Object.values(obj)[0]+'</td>';
            }
            html += html1+html2+'</tr>';
        }
        document.getElementById('TaskStateDa').innerHTML = html;
    }

