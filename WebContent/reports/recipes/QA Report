	//统计数据方法 
	function getDate(type,typeData,state){
		if("<%<%builtin Type%>%>" == type  && Projects[Projects.length-1] == "<%<%builtin Project%>%>"){ 
		for(var key in typeData){ 
			var arr = typeData[key];
			for(var i = 0;i<arr.length;i++){
				var obj = arr[i];
				if(state == Object.keys(obj)[0]){
					var count = parseInt(obj[state]);
					//console.log(count);
					obj[state] = count+1;
						
				}
				//console.log(obj);
			}
		}
		}
	}
	//报存统计数据
	data["<%<%builtin Project%>%>"].QATaskData = QATaskData;
	data["<%<%builtin Project%>%>"].ResponsibilityTeamData = ResponsibilityTeamData;
	data["<%<%builtin Project%>%>"].DocumentDate = DocumentDate;
	
	data["<%<%builtin Project%>%>"].SourceAndType = SourceAndType;
	data["<%<%builtin Project%>%>"].OriginAndTaskPhase = OriginAndTaskPhase;
	data["<%<%builtin Project%>%>"].StateAndDefectState = StateAndDefectState;
	data["<%<%builtin Project%>%>"].ResponsibilityTeamAndState = ResponsibilityTeamAndState;
	data["<%<%builtin Project%>%>"].ChangeRequestType = ChangeRequestType;
	data["<%<%builtin Project%>%>"].ChangeCategoryAndChangeInitiator = ChangeCategoryAndChangeInitiator;
	data["<%<%builtin Project%>%>"].ChangeOrderType = ChangeOrderType;
	data["<%<%builtin Project%>%>"].TestPlanType = TestPlanType;
	data["<%<%builtin Project%>%>"].TestObjectiveType = TestObjectiveType;
	data["<%<%builtin Project%>%>"].TestSessionType = TestSessionType;
	data["<%<%builtin Project%>%>"].TaskData = TaskData;
	data["<%<%builtin Project%>%>"].SupplierMonitorTaskData = SupplierMonitorTaskData;
	data["<%<%builtin Project%>%>"].ReleaseData = ReleaseData;
	data["<%<%builtin Project%>%>"].RiskData = RiskData;
	data["<%<%builtin Project%>%>"].LLAndBPData = LLAndBPData;
</script>
