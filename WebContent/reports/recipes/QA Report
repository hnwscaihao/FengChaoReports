    function getChangeRequestStateData(){
        for(var i=0;i<ChangeRequestStateData.length;i++){
            var obj = ChangeRequestStateData[i];
            var key = Object.keys(obj)[0];
            var count = parseInt(obj[key]);
            if("Open" == key){
                document.getElementById("OpenChangeRequest").innerHTML = count;
            }else if("CCB" == key){
                document.getElementById("CCBChangeRequest").innerHTML = count;
            }else if("Cancelled" == key){
                document.getElementById("CancelledChangeRequest").innerHTML = count;
            } else if("Postponed" == key){
                document.getElementById("PostponedChangeRequest").innerHTML = count;
            }else if("Approved" == key){
                document.getElementById("ApprovedChangeRequest").innerHTML = count;
            }else if("Fixed" == key){
                document.getElementById("FixedChangeRequest").innerHTML = count;
            }else if("Closed" == key){
                document.getElementById("ClosedChangeRequest").innerHTML = count;
            }else if("Total" == key){
                document.getElementById("TotalChangeRequest").innerHTML = count;
            }
        }
    };

    function getChangeRequestCategoryData(){
        var html = '';
        for(var key in ChangeRequestCategoryData){
            var html1 = '<tr class="odd_row" style="width: 100%;"><th style="text-align:right; width: 30%">'+key+ ':</th>';
